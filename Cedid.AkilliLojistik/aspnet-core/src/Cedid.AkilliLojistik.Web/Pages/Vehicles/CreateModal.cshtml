@page
@using Cedid.AkilliLojistik.Localization
@using Cedid.AkilliLojistik.Web.Pages.Vehicles
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<AkilliLojistikResource> L
@{ Layout = null; }

<abp-dynamic-form abp-model="Vehicle" asp-page="/Vehicles/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewVehicle"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>